    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.15.1",
    "react-navigation": "^4.4.4",
    "react-navigation-stack": "^2.10.4",
    "express": "^5.1.0",
    "mongoose": "^8.18.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@types/supertest": "^6.0.3",
    "@types/cors": "^2.8.19",
    "cross-env": "^10.0.0",
    "jest": "^29.7.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
