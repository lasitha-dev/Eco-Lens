{
  "name": "eco-lens",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "server:dev": "cross-env NODE_ENV=development ts-node src/index.ts",
