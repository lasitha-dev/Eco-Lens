    "server:test": "cross-env MONGODB_URL=$MONGODB_URL jest --passWithNoTests"
  },
  "dependencies": {
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.25",
    "expo": "~53.0.20",
